{
  "name": "pact-js-grpc-consumer",
  "version": "1.0.0",
  "description": "Pact JS gRPC Example Consumer",
  "scripts": {
    "clean": "rimraf pacts",
    "pretest": "npm run clean",
    "test": "npm run test:consumer",
    "test:consumer": "mocha test/*.consumer.spec.ts",
    "build": "npm run build:typescript",
    "build:typescript": "tsc -p .",
    "watch": "npm run build:typescript -- -w",
    "build:proto": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/ ./proto/area_calculator.proto",
    "pact:publish": "pact-broker publish ./pacts --consumer-app-version=\"1.0.0\" --auto-detect-version-properties --broker-base-url=http://127.0.0.1:8000 --broker-username pact_workshop --broker-password pact_workshop",
    "prewatch": "npm run build:proto",
    "prebuild": "npm run build:proto",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{ts,js,json,svg,md,yml}\"",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:eslint -- --fix",
    "lint:fix:prettier": "prettier --write '**/*.{ts,tsx,js,json,svg,md,yml}'",
    "start": "npm run start:consumer",
    "start:consumer": "ts-node consumer.ts"
  },
  "author": "you@saf.dev",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "1.14.0",
    "@grpc/proto-loader": "0.8.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^16.0.0",
    "@tsconfig/node16": "16.1.5",
    "@types/chai": "4.3.11",
    "@types/chai-as-promised": "7.1.8",
    "@types/mocha": "10.0.10",
    "@types/node": "24.5.2",
    "chai": "4.4.1",
    "chai-as-promised": "7.1.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "mocha": "10.8.2",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.js",
      "proto/**/*.ts"
    ]
  },
  "config": {
    "pact_do_not_track": true
  }
}
